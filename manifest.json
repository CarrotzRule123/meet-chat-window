{
    "manifest_version": 2,
    "name": "Google Meet Chat Window",
    "description": "Adds a separate chat window for Google Meet.",
    "version": "1.0.0",
    "author": "Jin Wei Tan",
    "content_scripts": [
    {
        "matches": ["https://meet.google.com/*"],
        "js": ["src/script.js"],
        "run_at": "document_idle"
    }
    ],
    "web_accessible_resources": ["src/popup/popup.html", "src/popup/style.css"]
}